{
  "aextends": [
    "tslint-config-prettier",
    "tslint-eslint-rules"
  ],
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "no-submodule-imports":false,
    "no-implicit-dependencies": false,
    "interface-over-type-literal": false,
    "member-access": [
      true,
      "no-public"
    ],
    "jsx-no-lambda": false,
    "no-console": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ]
  }
}